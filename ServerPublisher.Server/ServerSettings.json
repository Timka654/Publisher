{
  "publisher": {

    "server": {

      "io": {
        "port": 6583,
        "buffer_size": 409600,
        "backlog": 100
      },

      "cipher": {
        "input_key": "!{b1HX11R**",
        "output_key": "!{b1HX11R**"
      }

    },

    "project_configuration": {

      "base": {
        "ignore.file.paths": [ "Publisher/[\\s|\\S]*" ]
      },

      "default": {
        "ignore.file.paths": [ "appsettings.[\\s|\\S]*" ]
      },

      "server": {
        "scripts.default.usings": [
          "System",
          "System.IO",
          "System.Collections",
          "System.Collections.Generic",
          "System.Linq",
          "System.Diagnostics",
          "ServerPublisher.Server.Info",
          "ServerPublisher.Server.Scripts"
        ]
      }
    }
  }
}